{
    "name": "tediscript/laravel-view-generator",
    "description": "Laravel module to generate view from artisan command",
    "keywords": ["laravel", "view", "generator",  "module"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tediscript/laravel-view-generator/issues",
        "source": "https://github.com/tediscript/laravel-view-generator"
    },
    "authors": [
        {
            "name": "Tedi Fibri",
            "email": "tediscript@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/filesystem": "^8.42",
        "illuminate/support": "^8.42",
        "illuminate/validation": "^8.42"
    },
    "autoload": {
        "psr-4": {
            "Tediscript\\ViewGenerator\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1.0.0"
        },
        "laravel": {
            "providers": [
                "Tediscript\\ViewGenerator\\ViewGeneratorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
